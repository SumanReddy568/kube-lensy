  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "server": "tsx --watch server.ts",
    "server:build": "esbuild server.ts --bundle --platform=node --outfile=dist-server/server.js --external:express --external:cors && esbuild mcp-server.ts --bundle --platform=node --outfile=dist-server/mcp-server.js",
    "dev:all": "concurrently \"npm run server\" \"npm run dev\"",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "start": "npm run build && npm run server",
    "preview": "vite preview",
    "mcp-server": "tsx mcp-server.ts",
    "electron:dev": "concurrently \"npm run server\" \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build && npm run server:build && electron-builder"
  },
